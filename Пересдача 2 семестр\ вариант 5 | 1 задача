|function primeFactorization(number) {
let primeFactors = [];

// Разложение на "2" пока число четное
while (number % 2 === 0) {
primeFactors.push(2);
number /= 2;
}

// Поиск простых множителей, начиная с "3" до квадратного корня из числа
for (let i = 3; i <= Math.sqrt(number); i += 2) {
while (number % i === 0) {
primeFactors.push(i);
number /= i;
}
}

// Добавление оставшегося простого множителя (если он больше 2)
if (number > 2) {
primeFactors.push(number);
}

return primeFactors;
}

let number = 125;
let primeFactors = primeFactorization(number);

console.log(`Простые множители числа ${number}:`);
primeFactors.forEach(factor => {
console.log(factor);
});
